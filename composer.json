{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-db": "^2.8.1",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc-console": "^1.1.10",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugins": "^1.0.1",
        "zendframework/zend-psr7bridge": "^0.2.2",
        "zendframework/zend-session": "^2.7.1",
        "zendframework/zend-servicemanager-di": "^1.0",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-paginator": "^2.7",
        "zendframework/zend-navigation": "^2.8",
        "doctrine/doctrine-orm-module": "^1.1",
        "symfony/event-dispatcher": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/config": "^3.3",
        "symfony/finder": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/polyfill-apcu": "^1.4",
        "symfony/proxy-manager-bridge": "^3.3",
        "ocramius/generated-hydrator": "^2.0",
        "zendframework/zend-xmlrpc": "^2.6",
        "zendframework/zend-soap": "^2.6",
        "symfony/http-kernel": "^3.3",
        "symfony/browser-kit": "^3.3",
        "symfony/css-selector": "^3.3",
        "symfony/process": "^3.3",
        "symfony/class-loader": "^3.3",
        "symfony/var-dumper": "^3.3",
        "symfony/filesystem": "^3.3",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/migrations": "^1.5",
        "jdorn/sql-formatter": "^1.2",
        "zendframework/zendservice-recaptcha": "^3.0",
        "zendframework/zend-xml2json": "^3.0",
        "zendframework/zend-stratigility": "^2.0",
        "zendframework/zend-serializer": "^2.8",
        "zendframework/zend-mail": "^2.8",
        "zendframework/zend-ldap": "^2.8",
        "zendframework/zend-json-server": "^3.0",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-feed": "^2.8",
        "zendframework/zend-developer-tools": "^1.1",
        "zendframework/zend-crypt": "^3.2",
        "zendframework/zend-captcha": "^2.7",
        "mongodb/mongodb": "^1.1",
        "mongofill/mongofill": "dev-master",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-barcode": "^2.6",
        "zendframework/zendpdf": "^2.0",
        "zendframework/zend-file": "^2.7",
        "zendframework/zend-permissions-rbac": "^2.5",
        "zendframework/zend-text": "^2.6",
        "zendframework/zend-test": "^3.1",
        "zendframework/zend-http": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
						"User\\": "module/User/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test",
						"UserTest\\": "module/User/test"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
    }
}
